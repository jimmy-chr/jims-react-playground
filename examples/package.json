  "scripts": {
    "prebuild": "node ./bin/tools/create-version.js",
    "build": "webpack --config webpack.production.config.js --progress",
    "build:analyze": "npm run build -- --analyze",
    "lint:js": "eslint --cache ./src --ext .js,.ts,.tsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:style": "stylelint './src/**/*.{ts,tsx}'",
    "lint": "npm run lint:js && npm run lint:style",
    "typescript:check": "tsc --p ./tsconfig.app.json --noEmit",
    "clean:package-lock": "node ./bin/tools/clean-package-lock.js",
    "precommit": "npm run clean:package-lock && npm run typescript:check && lint-staged",
    "postinstall": "node ./bin/post-install.js",
    "start": "webpack serve --config ./webpack.config.js --mode development --progress  --color",
    "start:standalone": "concurrently --kill-others \"cross-env STANDALONE=1 npm run start\" \"node standalone\"",
    "build:standalone": "COLLECT_COVERAGE=1 npm run build && node standalone",
    "cy:run": "./node_modules/.bin/cypress run",
    "cy:run:record": "./node_modules/.bin/cypress run --record --key 9b8c2c04-338a-4622-bf6f-d568ed652515",
    "cy:open": "./node_modules/.bin/cypress open ",
    "cy:standalone:run": "start-server-and-test start:standalone http-get://localhost:8899/my. cy:run",
    "test": "npm run unit",
    "test:changed": "npm run unit -- --onlyChanged --coverage",
    "test:coverage": "npm run unit -- --coverage",
    "unit": "jest --maxWorkers=4",
    "unit:watch": "npm run unit -- --watch",
    "publish:ci": "curl -X POST 'https://${BB_AUTH_STRING}@api.bitbucket.org/2.0/repositories/${BITBUCKET_REPO_OWNER}/${BITBUCKET_REPO_SLUG}/downloads' --form files=@'public.zip'",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "generate-permissions": "node ./bin/tools/generate-permissions-overview.js",
    "version": "node ./bin/tools/create-version.js",
    "get-version": "echo $npm_package_version",
    "generate-ccam-testdata": "node bin/tools/test-data/ccam/test-data-generator.js",
    "generate-ccam-apis": "node ./bin/download-ccam-open-api-files.js && openapi-generator batch ./api-generators/*.yml",
    "eslint-check": "eslint --print-config ./src/index.tsx | eslint-config-prettier-check",
    "prepare": "husky install",
    "merge-coverage": "node ./bin/merge-code-coverage.js"
  },
